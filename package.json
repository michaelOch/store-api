{
  "name": "cherrystore",
  "version": "0.0.1",
  "description": "API for Cherry's Store  app",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets @babel/preset-env\"",
    "build": "babel src -s -D -d dist --presets @babel/preset-env",
    "start": "node ./dist/index.js",
    "stop": "NODE_ENV=production node dist --watch",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Tribesquare",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "axios": "^0.21.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "cloudinary": "^1.41.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^5.0.1",
    "ejs": "^2.6.2",
    "email-templates": "^4.0.1",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "fcm-push": "^1.1.3",
    "formidable": "^1.2.1",
    "fs": "0.0.1-security",
    "handlebars": "^4.0.11",
    "https": "^1.0.0",
    "i": "^0.3.6",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.1.1",
    "log": "^1.4.0",
    "md5": "^2.2.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.1",
    "multer": "^1.4.2",
    "node-forge": "^0.9.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.3.0",
    "nodemailer-express-handlebars": "^3.1.0",
    "npm": "^6.11.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.0",
    "path": "^0.12.7",
    "request": "^2.85.0",
    "socket-io": "^1.0.0",
    "socket.io": "^2.0.4",
    "stripe": "^8.132.0",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@types/node": "^12.7.11",
    "babel-eslint": "^8.2.2",
    "eslint": "^6.4.0",
    "nodemon": "^1.19.2"
  }
}
